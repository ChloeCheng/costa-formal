.progress-cover, .dialog-cover {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 200;
  background: rgba(0, 0, 0, 0.4);
}

.progress-dialog {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 201;
  transform: translate(-50%, -50%);
  width: 600rpx;
  background: transparent;
  border-radius: 10rpx;
  text-align: center;
}

.progress-dialog text {
  font-size: 32rpx;
  color: #949295;
  padding: 50rpx 24rpx;
  display: block;
  border-bottom: 1rpx solid #edebed;
}

.progress-dialog button {
  font-size: 32rpx;
  color: #3d3c40;
  background: #fff;
  border: 0;
  outline: none;
  border-radius: none;
  padding: 20rpx;
}

.progress-dialog button:visited, .progress-dialog button:focus {
  outline: none;
  border-radius: none;
  border: 1rpx solid transparent;
}

button.u-login-wx::after {
  border: 0;
}

.progress-block {
  position: relative;
  z-index: 100;
  background: white;
  border-radius: 10rpx;
  padding-bottom: 20px;
}

.icon-close {
  position: absolute;
  right: -30rpx;
  top: -70rpx;
  padding:0 20px;
  z-index:1;
}

.icon-close image {
  width: 40rpx;
  height: 80rpx;
}

.progress-bg {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}

.icon-logo {
  text-align: center;
  background: #9e0027;
  height: 80rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.icon-logo image {
  width: 220rpx;
  height: 80rpx;
  margin: 0;
  padding: 0;
}
.des-text-block{
  margin: 20px 0;
  font-size: 14px;
}

.btn-detail{
  background: #ebcb4c;
  width: 100px;
  padding: 4px 0;
  font-size: 13px;
  border-radius: 8rpx;
  margin: 0 auto;
}
.cover-image{
  width:100%;
  height: 100%;
}
.cover-image image{
  width:100%;
  height: 100%;
}